{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint-config-airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "rules": {
      "max-classes-per-file": 0,
      "class-methods-use-this": 0,
      "import/extensions": 0
    }
}